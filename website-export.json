        "page": 2,
        "type": "scores",
        "sections": [
          {
            "type": "header",
            "content": "Your Leadership Snapshot"
          },
          {
            "type": "scoreCards",
            "categories": [
              "Leadership & Team Alignment",
              "Time & Energy",
              "Talent & Retention",
              "EQ & Blind Spots",
              "Growth & Strategy",
              "Personal Fulfillment & Purpose"
            ],
            "scoreFormat": {
              "percentage": true,
              "level": ["Low", "Medium", "High"],
              "thresholds": {
                "high": 75,
                "medium": 50,
                "low": 0
